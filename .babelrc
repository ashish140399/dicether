{
  "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "entry",
            "corejs": 3,
            "targets": {
                "browsers": [">1%", "last 2 versions"]
            },
            "debug": false
        }],
        "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread", "@babel/plugin-proposal-class-properties"]
}
